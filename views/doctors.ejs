  <%-include('partials/headerPatient')%>
  
  	    
  <body style="background-color:#e6e9f0">
        <%-include('./partials/flash')%>
  	    <h1 class="text-center mb-5">DOCTORS</h1>
  	<main class="container mt-5">
    
  	
    
  	
  	
  		<%for(let d of doctors){%>
  		 <div class="card mb-5" style="box-shadow: 0 0 2rem rgba(0, 0, 0, 0.5); width: 500px;">
  		  
  		 			<img class="card-img-top" src="<%=d.img%>" style="height: 300px;">
  		 		
  		 		
  		 			<div class="card-body">
  		 			<h5 class="card-title" style="text-transform: uppercase; text-align: center;" >
  		 				<%=d.username%>
  		 			</h5>
  		 			<p class="card-text"><%=d.description%></p>
  		 			<p class="card-text"><small class="text-muted"><%=d.location%></small></p>
  		 			<a href="/doctors/<%=d.id%>" class="btn btn-dark">View <%=d.username%></a>
  		 		</div>
  		
  		
      </div>

  		<%}%>


</main>
